{
    "name": "limedocs/limedocs",
    "type": "library",
    "description": "A great PHP documentation tool for your APIs",
    "minimum-stability": "stable",
    "homepage": "http://limedocs.io",
    "keywords": ["doc", "documentation", "phpdoc", "apidoc", "apigen", "phpdocumentator"],
    "license": "MIT",
    "authors": [
        {
            "name": "Matthias ETIENNE",
            "email": "matthias@etienne.in"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "4.2.*",
        "phploc/phploc": "*",
        "phpmd/phpmd": "2.0.*",
        "squizlabs/php_codesniffer": "2.0.0RC1"
    },
    "require": {
        "symfony/console": "2.5.*",
        "monolog/monolog": "1.10.*",
        "symfony/event-dispatcher": "2.5.*",
        "symfony/dependency-injection": "2.5.*",
        "symfony/config": "2.5.*",
        "symfony/yaml": "2.5",
        "doctrine/cache": "~1.3.1",
        "michelf/php-markdown": "~1.4.0",
        "symfony/expression-language": "2.5.*"
    },
    "bin": [
        "bin/limedocs"
    ],
    "autoload": {
        "psr-4": {
            "Lime\\": "src/Lime",
            "Lime\\Test\\": "tests/"
        }
    }
}
